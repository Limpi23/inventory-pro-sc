{
  "name": "inventory-suit",
  "private": true,
  "version": "1.3.2",
  "type": "module",
  "main": "electron.cjs",
  "scripts": {
    "dev": "vite --port 8080",
    "dev:electron": "cross-env NODE_ENV=development concurrently \"vite --port 8080\" \"electron .\"",
    "build": "tsc --skipLibCheck --noEmit false && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "db:migration:new": "supabase migration new",
    "db:migration:apply": "supabase migration up",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:win": "electron-forge make"
  },
  "keywords": [
    "inventario",
    "electron",
    "vite",
    "react"
